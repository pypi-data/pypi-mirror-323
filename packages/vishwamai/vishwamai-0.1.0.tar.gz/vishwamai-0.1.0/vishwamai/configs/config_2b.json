{
    "model_type": "vishwamai",
    "architectures": ["VishwamaiModel"],
    "vocab_size": 64000,
    "hidden_size": 2048,
    "intermediate_size": 8192,
    "num_attention_heads": 24,
    "num_hidden_layers": 24,
    "max_position_embeddings": 8192,
    "tree_depth": 4,
    "branch_factor": 8,
    "use_conceptual_tokens": true,
    "concept_embedding_size": 256,
    "use_fp8": true,
    "attention_dropout": 0.1,
    "hidden_dropout": 0.1,
    "max_concepts_per_token": 4,
    "initializer_range": 0.02,
    "layer_norm_epsilon": 1e-5,
    "use_cache": true,
    "pad_token_id": 0,
    "bos_token_id": 1,
    "eos_token_id": 2,
    "tie_word_embeddings": true,
    "rope_scaling": {
        "type": "linear",
        "factor": 0.5
    },
    "attention_config": {
        "use_flash_attention": true,
        "attention_softmax_in_fp32": true,
        "fused_qkv": true
    },
    "training_config": {
        "learning_rate": 1e-4,
        "weight_decay": 0.1,
        "beta1": 0.9,
        "beta2": 0.95,
        "grad_clip": 1.0,
        "warmup_steps": 2000
    }
}
